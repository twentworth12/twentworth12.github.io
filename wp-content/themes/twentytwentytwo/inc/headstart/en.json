            "post_title": "Event",
            "post_content": "<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"left\":\"max(1.25rem, 5vw)\",\"right\":\"max(1.25rem, 5vw)\"}}},\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull\" style=\"padding-right:max(1.25rem,5vw);padding-left:max(1.25rem,5vw);\"><!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"5%\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"70%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:70%;\"><!-- wp:heading {\"level\":1,\"align\":\"wide\",\"style\":{\"typography\":{\"fontSize\":\"clamp(3rem, 6vw, 4.5rem)\"},\"spacing\":{\"margin\":{\"bottom\":\"0px\"}}}} -->\n<h1 class=\"alignwide\" id=\"flutter-a-collection-of-bird-related-ephemera-1\" style=\"font-size:clamp(3rem,6vw,4.5rem);margin-bottom:0;\"><em>Flutter<\/em>, a collection of bird-related ephemera<\/h1>\n<!-- \/wp:heading --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"\"} -->\n<div class=\"wp-block-column\"><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"5%\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"70%\",\"style\":{\"spacing\":{\"padding\":{\"bottom\":\"32px\"}}}} -->\n<div class=\"wp-block-column\" style=\"padding-bottom:32px;flex-basis:70%;\"><!-- wp:image {\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full\"><img src=\"https:\/\/2022.wordpress.net\/wp-content\/themes\/twentytwentytwo\/assets\/images\/bird-on-salmon.jpg\" alt=\"Image of a bird on a branch\" \/><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"\"} -->\n<div class=\"wp-block-column\"><!-- wp:image {\"width\":100,\"height\":47,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/2022.wordpress.net\/wp-content\/themes\/twentytwentytwo\/assets\/images\/icon-binoculars.png\" alt=\"An icon representing binoculars.\" width=\"100\" height=\"47\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:spacer {\"height\":16} -->\n<div style=\"height:16px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:heading {\"level\":3,\"fontSize\":\"medium\"} -->\n<h3 class=\"has-medium-font-size\" id=\"date-1\"><em>Date<\/em><\/h3>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>February, 12 2021<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:spacer {\"height\":16} -->\n<div style=\"height:16px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:heading {\"level\":3,\"fontSize\":\"medium\"} -->\n<h3 class=\"has-medium-font-size\" id=\"location-1\"><em>Location<\/em><\/h3>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>The Grand Theater<br>154 Eastern Avenue<br>Maryland NY, 12345<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:spacer {\"height\":16} -->\n<div style=\"height:16px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->",
